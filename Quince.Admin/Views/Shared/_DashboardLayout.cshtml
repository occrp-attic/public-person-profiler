@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <title>Quince.Admin panel</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css">
    <link href="~/Content/Dashboard/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Dashboard/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Dashboard/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Dashboard/assets/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Dashboard/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/global/plugins/bootstrap-modal/css/bootstrap-modal.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/global/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/global/plugins/bootstrap-datepicker/css/bootstrap-datepicker3.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/global/plugins/datatables/media/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/global/plugins/select2/select2.css" rel="stylesheet" type="text/css" />
    @*<link href="~/Content/Dashboard/assets/global/plugins/select2/select2-bootstrap.css" rel="stylesheet" type="text/css" />*@
    <link href="~/Content/Dashboard/assets/global/css/components-md.css" id="style_components" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/global/css/plugins-md.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/admin/css/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/admin/css/light.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/admin/css/custom.css" rel="stylesheet" type="text/css" />
    <script id="breadcrumbTemplate" type="text/x-handlebars-template">
        <div class="quince-tab-button">
            <a href="#{{windowId}}" data-window-id="{{windowId}}" id="{{breadcrumbId}}" class="btn btn-sm default quince-breadcrumb">
                {{text}}
            </a>
            <a href="javascript:;" data-window-id="#{{windowId}}" class="close">
                <i class="fa fa-close"></i>
            </a>
        </div>
    </script>
    <script id="windowTemplate" type="text/x-handlebars-template">
        <div class="portlet light bordered quince-window" id="{{windowId}}">
            <div class="portlet-title quince-title">
                <div class="caption font-green-sharp">
                    @*<i class="icon-speech font-green-sharp"></i>*@
                    <span class="caption-subject bold uppercase">{{text}}</span>
                </div>
                <div class="actions quince-actions">
                </div>
            </div>
            <div class="portlet-body quince-body">
                <div class="progress progress-striped active">
                    <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">

                    </div>
                </div>
            </div>
        </div>
    </script>
    <script id="modalTemplate" type="text/x-handlebars-template">
        <div class="modal fade quince-modal" tabindex="-1">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{{text}}</h4>
            </div>
            <div class="modal-body quince-body">
                <div class="progress progress-striped active">
                    <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                        <span class="sr-only">
                            Loading
                        </span>
                    </div>
                </div>
            </div>
            <div class="modal-footer quince-actions">

            </div>
        </div>
    </script>
    <script id="errorTemplate" type="text/x-handlebars-template">
        <div class="note note-danger note-shadow quince-note">
            <p>
                {{text}}
            </p>
        </div>
    </script>
    <script id="warningTemplate" type="text/x-handlebars-template">
        <div class="note note-warning note-shadow  quince-note">
            <p>
                {{text}}
            </p>
        </div>
    </script>
</head>
<body class="page-md page-header-fixed page-sidebar-closed-hide-logo page-sidebar-fixed page-footer-fixed">
    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    { }
    @Html.Partial("_Header")
    <div class="page-container">
        @Html.Partial("_Sidebar")
        @RenderBody()
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <div class="page-footer">
        <div class="page-footer-inner">
            2012 - @DateTime.Now.Year &copy; Quince Framework.
        </div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>
    <!-- END FOOTER -->
    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
    <!-- BEGIN CORE PLUGINS -->
    <!--[if lt IE 9]>
    <script src="~/Content/Dashboard/assets/global/plugins/respond.min.js"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/excanvas.min.js"></script>
    <![endif]-->
    <script src="~/Content/Dashboard/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/jquery.cokie.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/bootstrap-modal/js/bootstrap-modal.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/bootstrap-modal/js/bootstrap-modalmanager.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/plugins/select2/select2.js" type="text/javascript"></script>
    @* <script src="~/Content/Dashboard/assets/global/plugins/datatables/media/js/jquery.dataTables.js" type="text/javascript"></script>*@
    <script src="~/Content/Dashboard/assets/global/plugins/handlebars-v3.0.3.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="http://malsup.github.com/jquery.form.js"></script>
    <!-- END CORE PLUGINS -->
    <script src="~/Content/Dashboard/assets/global/scripts/metronic.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/admin/scripts/layout.js" type="text/javascript"></script>
    <script src="~/Content/Dashboard/assets/global/scripts/quince.js" type="text/javascript"></script>
    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout
            Quince.init(); // init demo features
        });
    </script>
    <!-- END JAVASCRIPTS -->
</body>

</html>
